module Click_Hydrogen_adc

include Click_Hydrogen_types

sub procedure hydrogen_adcInit() 
sub function  hydrogen_readADC() as uint32_t 

implements

sub procedure hydrogen_adcInit() 

    ADC_Init() 
    ADC_Set_Input_Channel(_ADC_CHANNEL_4) 

end sub

sub function  hydrogen_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC_Read(4) 
    result = readADC 

end sub

end.