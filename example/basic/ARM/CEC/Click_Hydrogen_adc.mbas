module Click_Hydrogen_adc

include Click_Hydrogen_types

sub procedure hydrogen_adcInit() 
sub function  hydrogen_readADC() as uint32_t 

implements

sub procedure hydrogen_adcInit() 

    ADC1_Init() 
    ADC1_Set_Input_Channel(0x01) 

end sub

sub function  hydrogen_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC1_Read(1) 
    result = readADC 

end sub

end.