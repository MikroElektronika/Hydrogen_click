unit Click_Hydrogen_adc;

uses Click_Hydrogen_types;

procedure hydrogen_adcInit(); 
function  hydrogen_readADC() :  uint32_t; 

implementation

procedure hydrogen_adcInit(); 
begin

    ADC_Init(); 

end;

function  hydrogen_readADC() :  uint32_t; 
var
    readADC : uint32_t;
begin

    readADC := ADC_Get_Sample(0); 
    result := readADC; 

end;

end.